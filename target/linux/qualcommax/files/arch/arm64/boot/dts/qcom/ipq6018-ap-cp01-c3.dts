/dts-v1/;

#include "ipq6018.dtsi"
#include "ipq6018-cp-cpu.dtsi"
#include "ipq6018-ess.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	model = "Arista AP-C200P";
	compatible = "arista,ap-cp01-c3", "qcom,ipq6018";

	aliases {
		serial0 = &blsp1_uart3;
		ethernet0 = &dp1;
		label-mac-device = &dp1;
		led-boot = &pwr;
		led-failsafe = &pwr;
		led-running = &pwr;
		led-upgrade = &pwr;
	};

	chosen {
		stdout-path = "serial0:115200n8";
		bootargs-append = " root=/dev/ubiblock0_1";
	};


	leds {
		compatible = "gpio-leds";

		pwr: pwr {
			color = <LED_COLOR_ID_ORANGE>;
			function = LED_FUNCTION_POWER;
			gpios = <&tlmm 28 GPIO_ACTIVE_HIGH>;
		};

		lan  {
			color = <LED_COLOR_ID_BLUE>;
			function = LED_FUNCTION_LAN;
			gpios = <&tlmm 29 GPIO_ACTIVE_HIGH>;
		};

		wlan2g  {
			color = <LED_COLOR_ID_BLUE>;
			function = LED_FUNCTION_WLAN;
			function-enumerator = <0>;
			gpios = <&tlmm 30 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "phy0radio";
		};

		wlan5g  {
			color = <LED_COLOR_ID_BLUE>;
			function = LED_FUNCTION_WLAN;
			function-enumerator = <1>;
			gpios = <&tlmm 31 GPIO_ACTIVE_HIGH>;
			linux,default-trigger = "phy1radio";
		};
	};
};

&blsp1_uart3 {
	pinctrl-0 = <&serial_3_pins>;
	pinctrl-names = "default";
	status = "okay";
};

&tlmm {
	mdio_pins: mdio-pins {
		mdc {
			pins = "gpio64";
			function = "mdc";
			drive-strength = <8>;
			bias-pull-up;
		};

		mdio {
			pins = "gpio65";
			function = "mdio";
			drive-strength = <8>;
			bias-pull-up;
		};
	};
};

&edma {
	status = "okay";
};

&dp1 {
		status = "okay";

		phy-handle = <&phy6>;
		phy-mode = "sgmii";
		label = "lan";
};

&mdio {
	status = "okay";
	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	reset-gpios = <&tlmm 75 GPIO_ACTIVE_LOW>;
	
	phy6: ethernet-phy@6 {
		reg = <0x6>;
	};
};

&switch {
	status = "okay";

	switch_lan_bmp = <ESS_PORT1>;
	switch_mac_mode = <MAC_MODE_SGMII_CHANNEL0>;

	qcom,port_phyinfo {
		port@1 {
			port_id = <1>;
			phy_address = <6>;
		};
	};
};


&spi {
		cs-select = <0x00>;
		dma-names = "tx", "rx";
		pinctrl-0 = <0x0f>;
		pinctrl-names = "default";
		spi-max-frequency = <0x2faf080>;
		reg = <0x78b5000 0x600>;
		status = "ok";
		
		flash@0 {
			density = <0x1000000>;
			sector-size = <0x10000>;
			spi-max-frequency = <0x2faf080>;
			compatible = "n25q128a11";
			linux,modalias = "m25p80", "n25q128a11";
			use-default-sizes;
			
			partitions {
			
				compatible = "fixed-partitions";
				
				partition@0 {
					label = "0:SBL1";
					reg = <0x00 0xc0000>;
				};

				partition@110000 {
					label = "0:QSEE";
					reg = <0x110000 0x1a0000>;
				};

				partition@1760000 {
					label = "CONFIG";
					reg = <0x1760000 0x400000>;
				};

				partition@1b60000 {
					label = "LOGGING";
					reg = <0x1b60000 0x100000>;
				};

				partition@1c60000 {
					label = "DEVCERT";
					reg = <0x1c60000 0x20000>;
				};

				partition@1c80000 {
					label = "RESERVED";
					reg = <0x1c80000 0x380000>;
				};

				partition@2b0000 {
					label = "0:QSEE_1";
					reg = <0x2b0000 0x1a0000>;
				};

				partition@450000 {
					label = "0:DEVCFG";
					reg = <0x450000 0x10000>;
				};

				partition@460000 {
					label = "0:DEVCFG_1";
					reg = <0x460000 0x10000>;
				};

				partition@470000 {
					label = "0:RPM";
					reg = <0x470000 0x40000>;
				};

				partition@4b0000 {
					label = "0:RPM_1";
					reg = <0x4b0000 0x40000>;
				};

				partition@4f0000 {
					label = "0:uboot_env";
					reg = <0x4f0000 0x10000>;
				};

				partition@500000 {
					label = "0:CDT_1";
					reg = <0x500000 0x10000>;
				};

				partition@510000 {
					label = "0:APPSBLENV";
					reg = <0x510000 0x10000>;
				};

				partition@520000 {
					label = "0:APPSBL";
					reg = <0x520000 0x100000>;
				};

				partition@620000 {
					label = "0:APPSBL_1";
					reg = <0x620000 0x100000>;
				};

				partition@720000 {
					label = "0:ART";
					reg = <0x720000 0x40000>;
				};

				partition@760000 {
					label = "firmware";
					reg = <0x760000 0x800000>;
				};

				partition@c0000 {
					label = "0:MIBIB";
					reg = <0xc0000 0x10000>;
				};

				partition@d0000 {
					label = "0:BOOTCONFIG";
					reg = <0xd0000 0x20000>;
				};

				partition@f0000 {
					label = "0:BOOTCONFIG1";
					reg = <0xf0000 0x20000>;
				};

				partition@f60000 {
					label = "HL_backup1";
					reg = <0xf60000 0x800000>;
				};
			};
		};
	};
};

&qpic_bam {
	status = "okay";
};

&qpic_nand {
	status = "okay";

	nand@0 {
		reg = <0>;

		nand-ecc-strength = <4>;
		nand-ecc-step-size = <512>;
		nand-bus-width = <8>;

		partitions {
			compatible = "qcom,smem-part";
			
			partition@0 {
				label = "rootfs";
				reg = <0x00 0x10000000>;
			};
			
			partition@10000000 {
				label = "rootfs_1";
				reg = <0x10000000 0x10000000>;
			};
		};
	};
};

&wifi {
	status = "okay";

	qcom,ath11k-calibration-variant = "Arista-C200P";
};